{
  "name": "cypresspractice",
  "version": "1.0.0",
  "description": "sample",
  "main": "index.js",
  "scripts": {
    "test": "node_modules\\.bin\\cypress run",
    "firefox": "npm run test -- --browser firefox",
    "chrome": "npm run test -- --browser chrome",
    "PatronHouseTests": "npm run test -- --spec \"cypress/Integration/Patrons_House/*\"",
    "recordDashboard": "npm run test -- --record --key 3f662b5c-153f-408a-971b-9dc7e4ea51a7 --reporter mochawesome",
    "CypressOpen": "cypress open",
    "clean:reports": "if exist cypress\\reports rmdir /q /s cypress\\reports && mkdir cypress\\reports && mkdir cypress\\reports\\mochawesome-report",
    "pretest": "npm run clean:reports",
    "merge-report": "mochawesome-merge cypress/reports/mochawesome-report/*.json > cypress/reports/AutomationReport.json",
    "generate-report": "marge cypress/reports/AutomationReport.json --reportDir ./ --inline",
    "after:tests": "npm run merge-report && npm run generate-report"
  },
  "author": "nagendra",
  "license": "ISC",
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.2.1",
    "csvtojson": "^2.0.10",
    "cy-verify-downloads": "^0.1.8",
    "cypress": "^10.6.0",
    "cypress-downloadfile": "^1.2.2",
    "cypress-drag-drop": "^1.1.1",
    "cypress-file-upload": "^5.0.8",
    "cypress-iframe": "^1.0.1",
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "node-xlsx": "^0.21.0"
  },
  "dependencies": {
    "pdf-parse": "^1.1.1"
  }
}